{
	"name": "yt-kabir-dohe-chrome-extension",
	"version": "1.2.1",
	"description": "A Chrome extension that automatically posts a random Kabir doha in the YouTube video comment box and likes the video. Simply click the extension icon while watching a YouTube video to activate.",
	"scripts": {
		"build:data": "node src/bin/build.js",
		"prebuild": "npm run build:data",
		"build": "webpack",
		"start": "webpack --watch",
		"lint": "eslint ./ --ext .js,.jsx",
		"lint:fix": "eslint ./ --ext .js,.jsx --fix"
	},
	"type": "module",
	"author": "Vijay Hardaha",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/eslint-parser": "^7.25.1",
		"@babel/preset-env": "^7.25.3",
		"babel-loader": "^9.1.3",
		"clean-webpack-plugin": "^4.0.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-jsx-a11y": "^6.10.0",
		"eslint-plugin-prettier": "^5.2.1",
		"prettier": "^3.3.3",
		"terser-webpack-plugin": "^5.3.10",
		"webpack": "^5.93.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"ora": "^8.0.1"
	}
}
